<div class="container-fluid">
<nav class="navbar navbar-toggleable-md navbar-light bg-faded">
  <div class="navbar-brand">Event Name</div>
</nav>
<div class="col-sm-10 offset-sm-1 mt-2">
<% if !@widget.widgetpictures.nil? %>
    <% @widget.widgetpictures.each do |f| %>
        <%= f %>
        <%= image_tag(f, class:"img-thumbnail", style:"height:10rem") %>
    <% end %>
<% end %>
<% if !@widget.widgetAttributes.nil? %>
    <% @widget.widgetAttributes.gsub(/(\[\"|\"\])/, '').split('", "').each do |f| %>
        <div><%= f %></div>
    <% end %>
<% end %>
</div>
</div>
