<div class="container-fluid">
<nav class="navbar navbar-toggleable-md navbar-light bg-faded">
  <div class="navbar-brand">Event Name</div>
</nav>
<div class="col-sm-10 offset-sm-1 mt-2">
<div class="container-fluid">
<div class="row">
    <%= form_for(@widget, url: widget_widget_images_path(@widget), method: :post) do |f| %>
        <%= f.file_field :images, multiple: true %>
        <%= f.submit "Add Pictures", class:"btn btn-primary" %> 
    <% end %>
    <div class="col-sm-4 mb-4 text-center"><button type="button" class="btn btn-primary">See Members</button></div>
</div>
<div class="row">
<% if !@widget.widgetpictures.nil? %>
    <% @widget.widgetpictures.each_with_index do |image, index| %>
        <%= link_to widget_widget_image_path(@widget, index) do %>
        <%= image_tag(image, class:"mb-2 ml-2 img-thumbnail", style:"height:10rem; height:10rem;") %>
        <% end %>
    <!--        
        <%= link_to "Delete Image", widget_widget_image_path(@widget, index), :method => :delete, data: { confirm: "Are you sure you want to delete this image?" } %> 
    -->
    <% end %>
<% end %>
<!--
<% if !@widget.widgetAttributes.nil? %>
    <% @widget.widgetAttributes.gsub(/(\[\"|\"\])/, '').split('", "').each do |f| %>
        <div><%= f %></div>
    <% end %>
<% end %>
-->
</div>
</div>
</div>
</div>
